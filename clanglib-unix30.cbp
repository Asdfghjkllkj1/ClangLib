<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="clanglib wx3.0.x (Unix)" />
		<Option pch_mode="2" />
		<Option default_target="ClangLib" />
		<Option compiler="gcc" />
		<Build>
			<Target title="release">
				<Option output="$(TARGET_NAME)/clanglib" prefix_auto="0" extension_auto="1" />
				<Option object_output=".objs/plugins/clanglib" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -ns -ni -v -p debug" />
				<Option host_application="./codeblocks" />
				<Option run_host_application_in_terminal="0" />
				<Option projectCompilerOptionsRelation="2" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-DBUILDING_PLUGIN" />
					<Add option="-DCB_PRECOMP" />
					<Add directory="." />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 clanglib.zip resources/manifest.xml resources/ccsettings.xrc resources/clangcodecompletion_toolbar.xrc resources/clang_project_settings_dlg.xrc" />
					<Add after="zip -j9 clanglib.cbplugin $(TARGET_NAME)/clanglib.so clanglib.zip" />
					<Add after="zip -r clanglib.cbplugin images/" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Target title="debug">
				<Option output="$(TARGET_NAME)/clanglib" prefix_auto="0" extension_auto="1" />
				<Option object_output=".objs/plugins/clanglib" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -ns -ni -v -p debug" />
				<Option host_application="./codeblocks" />
				<Option run_host_application_in_terminal="0" />
				<Option projectCompilerOptionsRelation="2" />
				<Compiler>
					<Add option="-g" />
					<Add option="-DBUILDING_PLUGIN" />
					<Add option="-DNOPCH" />
					<Add directory="." />
				</Compiler>
				<ExtraCommands>
					<Add after="zip -j9 clanglib.zip resources/manifest.xml resources/ccsettings.xrc resources/clangcodecompletion_toolbar.xrc resources/clang_project_settings_dlg.xrc" />
					<Add after="zip -j9 clanglib.cbplugin $(TARGET_NAME)/clanglib.so clanglib.zip" />
					<Add after="zip -r clanglib.cbplugin images/" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Environment>
				<Variable name="WX_CONFIG" value="wx-config --version=3.0" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="release;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wextra" />
			<Add option="-Wall" />
			<Add option="-std=c++11" />
			<Add option="-ansi" />
			<Add option="-fPIC" />
			<Add option="$(#CB_RELEASE_TYPE)" />
			<Add option="`$(WX_CONFIG) --cflags`" />
			<Add option="-I`llvm-config --includedir`" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-Winvalid-pch" />
			<Add option="-pthread" />
			<Add option="`pkg-config --cflags codeblocks`" />
			<Add option="-std=c++11" />
			<Add option="-DcbDEBUG" />
		</Compiler>
		<Linker>
			<Add option="`$(WX_CONFIG) --libs`" />
			<Add option="-L`llvm-config --libdir`" />
			<Add option="-Wl,--no-undefined" />
			<Add option="`pkg-config --libs codeblocks`" />
			<Add library="clang" />
		</Linker>
		<ExtraCommands>
			<Add before="echo libs: `pkg-config --libs codeblocks`" />
			<Add before="echo cflags: `pkg-config --cflags codeblocks`" />
		</ExtraCommands>
		<Unit filename="README.md">
			<Option target="&lt;{~None~}&gt;" />
		</Unit>
		<Unit filename="src/cclogger.cpp" />
		<Unit filename="src/cclogger.h" />
		<Unit filename="src/clangcc.cpp" />
		<Unit filename="src/clangcc.h" />
		<Unit filename="src/clangccsettingsdlg.cpp" />
		<Unit filename="src/clangccsettingsdlg.h" />
		<Unit filename="src/clangdiagnostics.cpp" />
		<Unit filename="src/clangdiagnostics.h" />
		<Unit filename="src/clangindexer.cpp" />
		<Unit filename="src/clangindexer.h" />
		<Unit filename="src/clangplugin.cpp" />
		<Unit filename="src/clangplugin.h" />
		<Unit filename="src/clangpluginapi.h" />
		<Unit filename="src/clangprojectsettingsdlg.cpp" />
		<Unit filename="src/clangprojectsettingsdlg.h" />
		<Unit filename="src/clangproxy.cpp" />
		<Unit filename="src/clangproxy.h" />
		<Unit filename="src/clangrefactoring.cpp" />
		<Unit filename="src/clangrefactoring.h" />
		<Unit filename="src/clangtoolbar.cpp" />
		<Unit filename="src/clangtoolbar.h" />
		<Unit filename="resources/ccsettings.xrc" />
		<Unit filename="resources/clangcodecompletion_toolbar.xrc" />
		<Unit filename="resources/manifest.xml" />
		<Unit filename="src/tokendatabase.cpp" />
		<Unit filename="src/tokendatabase.h" />
		<Unit filename="src/translationunit.cpp" />
		<Unit filename="src/translationunit.h" />
		<Unit filename="src/treemap.cpp" />
		<Unit filename="src/treemap.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
